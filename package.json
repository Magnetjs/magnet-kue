{
  "name": "magnet-kue",
  "version": "2.0.0",
  "description": "Magnet's adapter for Kue",
  "keywords": [
    "kue",
    "magnet",
    "queue"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-kue#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-kue/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Magnetjs/magnet-kue.git"
  },
  "scripts": {
    "--prepublish": "npm run build",
    "build": "npm run clean-lib && npm run build:index && npm run copy-package",
    "start": "babel src --out-dir dist",
    "test": "ava | tap-spec",
    "watch": "nodemon --ignore dist --exec \"babel src\" --out-dir dist",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "ava --watch | tap-spec",
    "build:index": "babel src --out-dir dist",
    "clean-lib": "node_modules/.bin/rimraf ./dist",
    "copy-package": "cp -rf package.json LICENSE README.md dist",
    "dist": "npm run build && cd dist && npm publish"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb"
  },
  "dependencies": {
    "kue": "0.x.x",
    "magnet-core": "^1.1.0",
    "mz": "^2.4.0",
    "require-all": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-transform-async-to-generator": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.1.0",
    "nodemon": "^1.9.1",
    "rimraf": "^2.5.4",
    "tap-spec": "^4.1.1"
  },
  "ava": {
    "failFast": true,
    "tap": true,
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
